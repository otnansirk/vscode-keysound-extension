{
    "name": "keysound",
    "displayName": "keysound",
    "description": "Tune Your Mechanical Keyboard Experience. Harmonize with Keysound.",
    "version": "1.0.0",
    "license": "MIT",
    "icon": "./assets/logo.gif",
    "engines": {
        "vscode": "^1.79.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "changelog": "./CHANGELOG.md",
        "configuration": {
            "title": "keysound",
            "properties": {
                "keysound.enabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "Controls whether the sound is enabled or disabled."
                },
                "keysound.other": {
                    "type": "string",
                    "default": true,
                    "description": "Controls to change sound. Use fullpath to file resource .wav"
                },
                "keysound.enter": {
                    "type": "string",
                    "default": true,
                    "description": "Controls to change enter sound. Use fullpath to file resource .wav"
                },
                "keysound.backspace": {
                    "type": "string",
                    "default": true,
                    "description": "Controls to change backspace sound. Use fullpath to file resource .wav"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "mocha",
        "cov": "nyc yarn test && istanbul-badges-readme"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "@types/vscode": "^1.79.0",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "@vscode/test-electron": "^2.3.2",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "istanbul-badges-readme": "^1.8.5",
        "mocha": "^10.2.0",
        "nyc": "^15.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "play-sound": "^1.1.5"
    },
    "keywords": [
        "keysound",
        "mechanical keyboard",
        "keyboard",
        "harmonize keyboard",
        "tool",
        "utility"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/otnansirk/vscode-keysound-extension.git"
    },
    "bugs": {
        "url": "https://github.com/otnansirk/vscode-keysound-extension/issues"
    }
}