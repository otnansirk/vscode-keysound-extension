{
    "name": "keysound",
    "displayName": "keysound",
    "description": "Tune Your Mechanical Keyboard Experience. Harmonize with Keysound.",
    "version": "0.0.1",
    "license": "MIT",
    "icon": "./assets/logo.gif",
    "engines": {
        "vscode": "^1.79.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "title": "keysound",
            "properties": {
                "keysound.enabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "Controls whether the sound is enabled or disabled."
                },
                "keysound.other": {
                    "type": "string",
                    "default": true,
                    "description": "Controls to change sound. Use fullpath to file resource .wav"
                },
                "keysound.enter": {
                    "type": "string",
                    "default": true,
                    "description": "Controls to change enter sound. Use fullpath to file resource .wav"
                },
                "keysound.backspace": {
                    "type": "string",
                    "default": true,
                    "description": "Controls to change backspace sound. Use fullpath to file resource .wav"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "@types/vscode": "^1.79.0",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "@vscode/test-electron": "^2.3.2",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "play-sound": "^1.1.5"
    }
}